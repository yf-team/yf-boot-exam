<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yf.modules.exam.tmpl.mapper.TmplMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yf.modules.exam.tmpl.dto.TmplDTO">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="cat_id" property="catId" />
        <result column="join_type" property="joinType" />
        <result column="qu_count" property="quCount" />
        <result column="total_score" property="totalScore" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        `id`,`title`,`cat_id`,`join_type`,`qu_count`,`total_score`
    </sql>

</mapper>
